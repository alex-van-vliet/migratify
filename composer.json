{
    "name": "alex-van-vliet/migratify",
    "description": "Laravel migrations creator from attributes",
    "authors": [
        {
            "name": "Alex van Vliet",
            "email": "alex@vanvliet.pro"
        }
    ],
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "AlexVanVliet\\Migratify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexVanVliet\\Migratify\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexVanVliet\\Migratify\\Providers\\MigratifyServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit":  "^9"
    },
    "scripts": {
        "test": "phpunit",
        "test-f": "phpunit --filter"
    }
}
